package shawty;

public class passwd {

	private int is_let(char c) {
		if (c >= 'a' && c <= 'z') return 1; 
		if (c >= 'A' && c <= 'Z') return 2;
		return 0;
	}
	
	private boolean is_num(char c) {
		return c >= '0' && c <= '9';
	}
	
	
	
	final private int min_len = 8;
	
	boolean has_num,
			has_low,
			has_upp,
			has_spe,
			has_len;
	
	private void prÃ¼fePasswortSicherheit(String passwort) {
		int nll, nul, nn, ns;
		has_num = has_low = has_upp = has_spe = has_len = false;
		nll = nul = nn = ns = 0;
		has_len = passwort.length() >= min_len;
		
		for (int i = 0; i < passwort.length(); i++)
		{
			nn += is_num(passwort.charAt(i));
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
